{
  "welcomeFile": "AppRouterHome.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/take-me/to-sap/(.*)$",
      "target": "$1",
      "destination": "env_destination_saphome"
    },
    {
      "source": "^/take-me/to-youtube/(.*)$",
      "target": "$2",
      "destination": "env_destination_youtubehome"
    }
  ]
}