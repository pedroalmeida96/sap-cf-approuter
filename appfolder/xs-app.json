      "authenticationType": "xsuaa",
      "scope": [
        "$XSAPPNAME.homepageAccessScope"
      ],
      "source": "^/take-me/to-sap/(.*)$",
      "target": "$1",
      "destination": "env_destination_saphome"
    }
  ]
}